{
  "name": "@vodding/client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "tsc -b && bunx --bun vite build",
    "lint:fix": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "format:fix": "prettier --write .",
    "preview": "bunx --bun vite preview"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bun run lint:fix",
      "bun run format:fix"
    ]
  },
  "dependencies": {
    "@vodding/common": "workspace:*",
    "@nextui-org/react": "^2.4.6",
    "framer-motion": "^11.3.28",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-player": "^2.16.0",
    "react-router-dom": "^6.26.1",
    "socket.io-client": "^4.7.5",
    "video.js": "^8.17.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/socket.io-client": "^3.0.0",
    "@types/video.js": "^7.3.58",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.10",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  }
}
